
public class HanoiR {
    public static Stack1Gen <FactRecord> s = new Stack1Gen<FactRecord>();

	public static void main(String args[]) {
		 s.push(new FactRecord(3));

	        while(!s.isEmpty()) 
	        {  // system loop goes until stack is empty
	            s.top().run();
	        }  // end system loop
	} // main method
}
